package javalab;
import java.util.Scanner;
public class Frequency
   {

	public static void main(String[] args)
	{
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the string");
	String str=sc.nextLine();
	System.out.println("to check the frequency of:");
	char ch=sc.nextLine().charAt(0);
	int len=str.length();
	int count=0;
	for(int i=0;i<len;i++)
	{
		if(str.charAt(i)==ch)
		{
			count++;
		}
	}
	System.out.println("The frequency of character "+ch+" in the string is:"+count);
	}
    }
